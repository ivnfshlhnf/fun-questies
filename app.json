{
  "expo": {
    "name": "fun-questies",
    "slug": "fun-questies",
    "scheme": "fun-questies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.panhanif.funquesties"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.panhanif.funquesties"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "build": {
        "babel": {
          "include": ["@ui-kitten/components"]
        }
      },
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "1fdb2aa3-e72b-4e65-86f3-004226536845"
      }
    },
    "owner": "panhanif"
  }
}
